{
  "build": {
    "env": {
      "ENABLE_ROOT_PATH_BUILD_CACHE": "1"
    }
  },
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/picture/:id",
      "destination": "/feed/:id"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/XiaoMengXinX/FuckCoolapkFeeds",
      "permanent": false
    },
    {
      "source": "/((?!api|feed|_next|picture).*)",
      "destination": "https://www.coolapk.com/$1",
      "permanent": false
    }
  ]
}